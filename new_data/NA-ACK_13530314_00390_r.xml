            <TextLine id="r1l2" custom="readingOrder {index:0;} person {offset:4; length:89;} place {offset:88; length:5;} person {offset:95; length:56;} place {offset:116; length:10;} place {offset:131; length:5;} place {offset:144; length:7;} person {offset:153; length:18; continued:true;}">
                <Coords points="1477,980 1850,987 2148,988 2446,989 2744,989 3042,989 3340,988 3638,987 3936,985 4234,984 4532,981 4829,979 5127,976 5425,973 5723,970 6021,966 6319,962 6617,959 6915,955 7213,951 7511,947 7526,613 7228,617 6930,619 6632,623 6347,620 6049,624 5748,620 5450,623 5191,622 4893,625 4557,622 4259,625 3966,622 3668,624 3372,625 3074,626 2797,624 2499,624 2157,632 1853,630 1480,623"/>
                <Baseline points="1553,961 1850,963 2148,964 2446,965 2744,965 3042,965 3340,964 3638,963 3936,961 4234,960 4532,957 4829,955 5127,952 5425,949 5723,946 6021,942 6319,938 6617,935 6915,931 7213,927 7511,923"/>
                <TextEquiv>
                    <Unicode>Wir Karl von Gots gnaden romischer kunig, ze allen zeiten merer des reichs und kunig ze Behem, Albrecht, hertzog ze Osterreich, ze Steyr und ze Kernden, Johans, margraf ze</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l3" custom="readingOrder {index:1;} person {offset:0; length:7; continued:true;} place {offset:0; length:7;} person {offset:13; length:57;}">
                <Coords points="1546,1090 1847,1091 2148,1092 2449,1093 2750,1093 3051,1093 3352,1092 3653,1091 3954,1090 4255,1089 4557,1087 4858,1084 5159,1082 5460,1079 5761,1076 6062,1072 6363,1067 6664,1063 6965,1058 7266,1052 7568,1046 7568,974 7266,980 6965,986 6664,991 6363,995 6062,1000 5761,1004 5460,1007 5159,1010 4858,1012 4557,1015 4255,1017 3954,1018 3653,1019 3352,1020 3051,1021 2750,1021 2449,1021 2148,1020 1847,1019 1546,1018"/>
                <Baseline points="1546,1066 1847,1067 2148,1068 2449,1069 2750,1069 3051,1069 3352,1068 3653,1067 3954,1066 4255,1065 4557,1063 4858,1060 5159,1058 5460,1055 5761,1052 6062,1048 6363,1043 6664,1039 6965,1034 7266,1028 7568,1022"/>
                <TextEquiv>
                    <Unicode>Merhern, und Rudolf, des obgenanten hertzog Albrecht, erstgeborner sun, von denselben Gots gnaden, vor uns selber und alle unser bruder, bechennen und tun chund offenlich mit disem</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l4" custom="readingOrder {index:2;}">
                <Coords points="1556,1195 1857,1197 2158,1199 2459,1200 2760,1200 3061,1199 3362,1198 3663,1196 3964,1194 4265,1191 4566,1187 4867,1184 5168,1180 5469,1176 5770,1172 6071,1168 6372,1165 6673,1161 6974,1158 7275,1155 7576,1153 7576,1081 7275,1083 6974,1086 6673,1089 6372,1093 6071,1096 5770,1100 5469,1104 5168,1108 4867,1112 4566,1115 4265,1119 3964,1122 3663,1124 3362,1126 3061,1127 2760,1128 2459,1128 2158,1127 1857,1125 1556,1123"/>
                <Baseline points="1556,1171 1857,1173 2158,1175 2459,1176 2760,1176 3061,1175 3362,1174 3663,1172 3964,1170 4265,1167 4566,1163 4867,1160 5168,1156 5469,1152 5770,1148 6071,1144 6372,1141 6673,1137 6974,1134 7275,1131 7576,1129"/>
                <TextEquiv>
                    <Unicode>brief allen den, die in sehent oder horent lesen, daz wir durch angeborne liebe luter, trewe und stete, freuntschaft, die von gemeinscheffte der geburde und des geslechts alleweg</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l5" custom="readingOrder {index:3;}">
                <Coords points="1564,1298 1859,1302 2154,1304 2450,1305 2745,1306 3041,1305 3336,1303 3631,1301 3927,1299 4222,1295 4518,1291 4813,1287 5108,1283 5404,1279 5699,1275 5995,1271 6290,1266 6585,1263 6881,1260 7176,1257 7472,1255 7472,1182 7176,1184 6881,1187 6585,1190 6290,1193 5995,1198 5699,1202 5404,1206 5108,1210 4813,1214 4518,1218 4222,1222 3927,1226 3631,1228 3336,1230 3041,1232 2745,1233 2450,1232 2154,1231 1859,1229 1564,1225"/>
                <Baseline points="1564,1274 1859,1278 2154,1280 2450,1281 2745,1282 3041,1281 3336,1279 3631,1277 3927,1275 4222,1271 4518,1267 4813,1263 5108,1259 5404,1255 5699,1251 5995,1247 6290,1242 6585,1239 6881,1236 7176,1233 7472,1231"/>
                <TextEquiv>
                    <Unicode>zwischen uns gewesen sind, und mit hilff Gots sullen ewichlich unverrukcht in freuntlicher guter sazze beleiben, ze sunderlichem troste und gnaden unser getrewn undertanen, uns allesampt</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l6" custom="readingOrder {index:4;}">
                <Coords points="1548,1393 1847,1399 2147,1403 2447,1405 2746,1406 3046,1406 3346,1405 3645,1403 3945,1400 4245,1396 4545,1392 4844,1387 5144,1383 5444,1377 5743,1372 6043,1367 6343,1362 6642,1357 6942,1353 7242,1350 7542,1347 7542,1278 7242,1281 6942,1284 6642,1288 6343,1293 6043,1298 5743,1303 5444,1308 5144,1314 4844,1318 4545,1323 4245,1327 3945,1331 3645,1334 3346,1336 3046,1337 2746,1337 2447,1336 2147,1334 1847,1330 1548,1324"/>
                <Baseline points="1548,1370 1847,1376 2147,1380 2447,1382 2746,1383 3046,1383 3346,1382 3645,1380 3945,1377 4245,1373 4545,1369 4844,1364 5144,1360 5444,1354 5743,1349 6043,1344 6343,1339 6642,1334 6942,1330 7242,1327 7542,1324"/>
                <TextEquiv>
                    <Unicode>mit wolbedachtem mute, mit rate unserr fursten, herrn und ouch getrewn mit rechter wizzen, zesamme verbunden haben und verpinden uns vor uns und unser erben ewichlich in guten</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l7" custom="readingOrder {index:5;}">
                <Coords points="1574,1489 1869,1494 2164,1497 2459,1499 2754,1500 3049,1500 3344,1499 3640,1498 3935,1495 4230,1492 4525,1489 4820,1485 5115,1481 5410,1476 5706,1472 6001,1467 6296,1462 6591,1457 6886,1453 7181,1448 7477,1444 7477,1375 7181,1379 6886,1384 6591,1388 6296,1393 6001,1398 5706,1403 5410,1407 5115,1412 4820,1416 4525,1420 4230,1423 3935,1426 3640,1429 3344,1430 3049,1431 2754,1431 2459,1430 2164,1428 1869,1425 1574,1420"/>
                <Baseline points="1574,1466 1869,1471 2164,1474 2459,1476 2754,1477 3049,1477 3344,1476 3640,1475 3935,1472 4230,1469 4525,1466 4820,1462 5115,1458 5410,1453 5706,1449 6001,1444 6296,1439 6591,1434 6886,1430 7181,1425 7477,1421"/>
                <TextEquiv>
                    <Unicode>trewn an geverde und mit dem eyde, den wir allesampt daruber gesworen haben, uf den heiligen ewangelien und geloben in chreften derselben verpuntnuzze in guten, steten, trewen, daz</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l8" custom="readingOrder {index:6;}">
                <Coords points="1564,1582 1859,1589 2154,1594 2450,1598 2745,1599 3041,1600 3336,1600 3632,1598 3927,1595 4223,1592 4518,1588 4813,1584 5109,1579 5404,1574 5700,1569 5995,1563 6291,1558 6586,1554 6882,1550 7177,1546 7473,1544 7473,1474 7177,1476 6882,1480 6586,1484 6291,1488 5995,1493 5700,1499 5404,1504 5109,1509 4813,1514 4518,1518 4223,1522 3927,1525 3632,1528 3336,1530 3041,1530 2745,1529 2450,1528 2154,1524 1859,1519 1564,1512"/>
                <Baseline points="1564,1559 1859,1566 2154,1571 2450,1575 2745,1576 3041,1577 3336,1577 3632,1575 3927,1572 4223,1569 4518,1565 4813,1561 5109,1556 5404,1551 5700,1546 5995,1540 6291,1535 6586,1531 6882,1527 7177,1523 7473,1521"/>
                <TextEquiv>
                    <Unicode>wir indwendig den kreizzen und gemerken unserr kunigreich furstentume und herschefte, die uns von erbschaft angehorent, und die wir ytzund innehaben oder mit hilfe Gots</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l9" custom="readingOrder {index:7;}">
                <Coords points="1586,1681 1881,1689 2177,1695 2472,1699 2768,1702 3064,1702 3359,1702 3655,1700 3951,1698 4246,1694 4542,1689 4838,1685 5133,1679 5429,1673 5725,1667 6020,1661 6316,1655 6612,1649 6907,1644 7203,1638 7499,1634 7499,1562 7203,1566 6907,1572 6612,1577 6316,1583 6020,1589 5725,1595 5429,1601 5133,1607 4838,1613 4542,1617 4246,1622 3951,1626 3655,1628 3359,1630 3064,1630 2768,1630 2472,1627 2177,1623 1881,1617 1586,1609"/>
                <Baseline points="1586,1657 1881,1665 2177,1671 2472,1675 2768,1678 3064,1678 3359,1678 3655,1676 3951,1674 4246,1670 4542,1665 4838,1661 5133,1655 5429,1649 5725,1643 6020,1637 6316,1631 6612,1625 6907,1620 7203,1614 7499,1610"/>
                <TextEquiv>
                    <Unicode>furbaz noch gewinnen, einander getrewlich und freuntlich beholfen sein wellen und sullen wider allermenilich, nymant auzgenomen, und in den zeiten, so eins oder mer wenn eins wider</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l10" custom="readingOrder {index:8;}">
                <Coords points="1572,1781 1870,1788 2168,1793 2467,1796 2765,1798 3064,1798 3362,1797 3661,1795 3959,1792 4258,1788 4556,1783 4854,1778 5153,1772 5451,1766 5750,1759 6048,1753 6347,1747 6645,1741 6944,1735 7242,1730 7541,1726 7541,1662 7242,1666 6944,1671 6645,1677 6347,1683 6048,1689 5750,1695 5451,1702 5153,1708 4854,1714 4556,1719 4258,1724 3959,1728 3661,1731 3362,1733 3064,1734 2765,1734 2467,1732 2168,1729 1870,1724 1572,1717"/>
                <Baseline points="1572,1760 1870,1767 2168,1772 2467,1775 2765,1777 3064,1777 3362,1776 3661,1774 3959,1771 4258,1767 4556,1762 4854,1757 5153,1751 5451,1745 5750,1738 6048,1732 6347,1726 6645,1720 6944,1714 7242,1709 7541,1705"/>
                <TextEquiv>
                    <Unicode>uns kunigreich, furstentum, herschefte und lande veintlich angriffen werden, so sullen und wellen wir andern zehant, alz wir dez ermanet werden, an ufschub und an allz verziehen</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l11" custom="readingOrder {index:9;}">
                <Coords points="1578,1868 1874,1878 2171,1887 2468,1892 2765,1896 3062,1898 3359,1897 3655,1896 3952,1893 4249,1889 4546,1884 4843,1878 5140,1871 5437,1865 5733,1859 6030,1852 6327,1846 6624,1840 6921,1834 7218,1830 7515,1827 7515,1763 7218,1766 6921,1770 6624,1776 6327,1782 6030,1788 5733,1795 5437,1801 5140,1807 4843,1814 4546,1820 4249,1825 3952,1829 3655,1832 3359,1833 3062,1834 2765,1832 2468,1828 2171,1823 1874,1814 1578,1804"/>
                <Baseline points="1578,1847 1874,1857 2171,1866 2468,1871 2765,1875 3062,1877 3359,1876 3655,1875 3952,1872 4249,1868 4546,1863 4843,1857 5140,1850 5437,1844 5733,1838 6030,1831 6327,1825 6624,1819 6921,1813 7218,1809 7515,1806"/>
                <TextEquiv>
                    <Unicode>dem oder denselben mit leib oder mit gute rat, hilf, gunst und bruderlich liebe mit aller macht beygestendig und geholffen sein uf unsern aigen schaden und verlust, die weil</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l12" custom="readingOrder {index:10;}">
                <Coords points="1548,1965 1848,1977 2149,1985 2449,1992 2750,1997 3051,1999 3351,1999 3652,1998 3952,1996 4253,1992 4554,1987 4854,1981 5155,1975 5455,1968 5756,1962 6057,1955 6357,1948 6658,1942 6958,1936 7259,1932 7560,1928 7560,1858 7259,1862 6958,1866 6658,1872 6357,1878 6057,1885 5756,1892 5455,1898 5155,1905 4854,1911 4554,1917 4253,1922 3952,1926 3652,1928 3351,1929 3051,1929 2750,1927 2449,1922 2149,1915 1848,1907 1548,1895"/>
                <Baseline points="1548,1942 1848,1954 2149,1962 2449,1969 2750,1974 3051,1976 3351,1976 3652,1975 3952,1973 4253,1969 4554,1964 4854,1958 5155,1952 5455,1945 5756,1939 6057,1932 6357,1925 6658,1919 6958,1913 7259,1909 7560,1905"/>
                <TextEquiv>
                    <Unicode>wir leben wider einen iglichen menschen und persone in welichen eren oder wesen, der sey und ob er ouch in kuniglichen wirden were, nymant auzzenemen, alz da vor begriffen</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l13" custom="readingOrder {index:11;} place {offset:35; length:6;}">
                <Coords points="1540,2069 1840,2081 2141,2090 2441,2096 2742,2100 3042,2102 3343,2102 3643,2101 3944,2097 4244,2093 4545,2087 4845,2081 5146,2074 5446,2067 5747,2059 6047,2052 6348,2044 6648,2038 6949,2032 7249,2026 7550,2022 7550,1950 7249,1954 6949,1960 6648,1966 6348,1972 6047,1980 5747,1987 5446,1995 5146,2002 4845,2009 4545,2015 4244,2021 3944,2025 3643,2029 3343,2030 3042,2030 2742,2028 2441,2024 2141,2018 1840,2009 1540,1997"/>
                <Baseline points="1540,2045 1840,2057 2141,2066 2441,2072 2742,2076 3042,2078 3343,2078 3643,2077 3944,2073 4244,2069 4545,2063 4845,2057 5146,2050 5446,2043 5747,2035 6047,2028 6348,2020 6648,2014 6949,2008 7249,2002 7550,1998"/>
                <TextEquiv>
                    <Unicode>ist, auzzenemen doch den kunig von Ungern, wann wir mit dem in allen den punden beleiben sullen, alz die brif sagent, die wir obgenanter romischer kunig, die weil wir margraf</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l14" custom="readingOrder {index:12;} place {offset:3; length:7;} person {offset:72; length:14;} place {offset:80; length:6;} person {offset:95; length:22;} place {offset:105; length:5;} person {offset:126; length:26;} place {offset:142; length:10;} person {offset:172; length:7; continued:true;}">
                <Coords points="1542,2166 1842,2179 2142,2190 2442,2198 2742,2203 3042,2206 3342,2207 3642,2206 3942,2204 4242,2200 4543,2195 4843,2190 5143,2183 5443,2176 5743,2169 6043,2161 6343,2154 6643,2146 6943,2141 7243,2135 7544,2131 7544,2059 7243,2063 6943,2069 6643,2074 6343,2082 6043,2089 5743,2097 5443,2104 5143,2111 4843,2118 4543,2123 4242,2128 3942,2132 3642,2134 3342,2135 3042,2134 2742,2131 2442,2126 2142,2118 1842,2107 1542,2094"/>
                <Baseline points="1542,2142 1842,2155 2142,2166 2442,2174 2742,2179 3042,2182 3342,2183 3642,2182 3942,2180 4242,2176 4543,2171 4843,2166 5143,2159 5443,2152 5743,2145 6043,2137 6343,2130 6643,2122 6943,2117 7243,2111 7544,2107"/>
                <TextEquiv>
                    <Unicode>ze Merhern waren, und seliger gedechtnuzze die hochgebornen fursten her Karl ze Ungern und her Johans ze Behem kunige und her Ott, hertzog ze Osterreich und wir obgenanter hertzog</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l15" custom="readingOrder {index:13;} person {offset:0; length:8; continued:true;} person {offset:34; length:56;} place {offset:54; length:7;} place {offset:85; length:5;}">
                <Coords points="1539,2278 1836,2291 2134,2300 2432,2308 2730,2313 3028,2315 3326,2316 3624,2315 3922,2312 4220,2308 4518,2302 4815,2297 5113,2290 5411,2283 5709,2275 6007,2267 6305,2259 6603,2252 6901,2245 7199,2239 7497,2234 7497,2161 7199,2166 6901,2172 6603,2179 6305,2186 6007,2194 5709,2202 5411,2210 5113,2217 4815,2224 4518,2229 4220,2235 3922,2239 3624,2242 3326,2243 3028,2242 2730,2240 2432,2235 2134,2227 1836,2218 1539,2205"/>
                <Baseline points="1539,2254 1836,2267 2134,2276 2432,2284 2730,2289 3028,2291 3326,2292 3624,2291 3922,2288 4220,2284 4518,2278 4815,2273 5113,2266 5411,2259 5709,2251 6007,2243 6305,2235 6603,2228 6901,2221 7199,2215 7497,2210"/>
                <TextEquiv>
                    <Unicode>Albrecht und ouch wir, obgenanter Johans, margrafe ze Merhern, in den zeiten graf ze Tirol, einander geben haben, wann diselb brif und punde unverrukchet und unzebrochen beleiben,</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l16" custom="readingOrder {index:14;}">
                <Coords points="1600,2378 1892,2394 2184,2405 2476,2414 2769,2419 3061,2422 3353,2423 3646,2421 3938,2418 4230,2414 4523,2407 4815,2401 5107,2393 5399,2385 5692,2378 5984,2370 6276,2364 6569,2357 6861,2352 7153,2349 7446,2347 7446,2274 7153,2276 6861,2279 6569,2284 6276,2291 5984,2297 5692,2305 5399,2312 5107,2320 4815,2328 4523,2334 4230,2341 3938,2345 3646,2348 3353,2350 3061,2349 2769,2346 2476,2341 2184,2332 1892,2321 1600,2305"/>
                <Baseline points="1600,2354 1892,2370 2184,2381 2476,2390 2769,2395 3061,2398 3353,2399 3646,2397 3938,2394 4230,2390 4523,2383 4815,2377 5107,2369 5399,2361 5692,2354 5984,2346 6276,2340 6569,2333 6861,2328 7153,2325 7446,2323"/>
                <TextEquiv>
                    <Unicode>sullen in solicher beschaidenhait, waz vesten lande, burge und stette in des landen gewunnen werden, der unserr hilff begeret, daz die demselben ungehindert beleiben sullen,</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l17" custom="readingOrder {index:15;}">
                <Coords points="1578,2488 1873,2503 2169,2514 2464,2522 2760,2528 3056,2531 3351,2532 3647,2530 3942,2528 4238,2523 4534,2518 4829,2511 5125,2504 5420,2496 5716,2489 6012,2481 6307,2474 6603,2467 6898,2462 7194,2458 7490,2455 7490,2382 7194,2385 6898,2389 6603,2394 6307,2401 6012,2408 5716,2416 5420,2423 5125,2431 4829,2438 4534,2445 4238,2450 3942,2455 3647,2457 3351,2459 3056,2458 2760,2455 2464,2449 2169,2441 1873,2430 1578,2415"/>
                <Baseline points="1578,2464 1873,2479 2169,2490 2464,2498 2760,2504 3056,2507 3351,2508 3647,2506 3942,2504 4238,2499 4534,2494 4829,2487 5125,2480 5420,2472 5716,2465 6012,2457 6307,2450 6603,2443 6898,2438 7194,2434 7490,2431"/>
                <TextEquiv>
                    <Unicode>aber varunde hab und gevangen mag und sol unserr iglicher ze seinem nutze behalten, der si mit seiner arbaitt erchrieget und gewinnet und in welichs hilff wir sein,</Unicode>
                </TextEquiv>
            </TextLine>
