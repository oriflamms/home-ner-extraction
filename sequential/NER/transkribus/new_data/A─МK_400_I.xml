                <zone points="1260,3824 1597,3822 1935,3820 2273,3818 2611,3817 2949,3816 3287,3815 3624,3815 3962,3814 4300,3813 4638,3812 4976,3810 5314,3808 5651,3805 5989,3801 6327,3796 6665,3791 7003,3784 7341,3776 7763,3767 7763,3713 7341,3722 7003,3730 6665,3737 6327,3742 5989,3747 5651,3751 5314,3754 4976,3756 4638,3758 4300,3759 3962,3760 3624,3761 3287,3761 2949,3762 2611,3763 2273,3764 1935,3766 1597,3768 1260,3770" rendition="Line" xml:id="facs_1_r1l82"/>
